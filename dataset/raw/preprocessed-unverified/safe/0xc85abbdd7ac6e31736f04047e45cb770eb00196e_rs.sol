// File: SimpleCollectible.sol

contract SimpleCollectible is ERC721 {
    uint256 public tokenCounter;
    mapping(uint => string) public tokenIdToDescription;


    constructor () public ERC721 ("Timeshard", "TIME"){
        tokenCounter = 0;
    }

    function createCollectible(string memory description) public returns (uint256) {
        uint256 newItemId = tokenCounter;
        _safeMint(msg.sender, newItemId);
        _setTokenURI(newItemId, uint2str(block.timestamp));
        tokenIdToDescription[newItemId] = description;
        tokenCounter = tokenCounter + 1;
        return newItemId;
    }

    function uint2str(uint _i) internal pure returns (string memory unix_time) {
            if (_i == 0) {
                return "0";
            }
            uint j = _i;
            uint len;
            while (j != 0) {
                len++;
                j /= 10;
            }
            bytes memory bstr = new bytes(len);
            uint k = len;
            while (_i != 0) {
                k = k-1;
                uint8 temp = (48 + uint8(_i - _i / 10 * 10));
                bytes1 b1 = bytes1(temp);
                bstr[k] = b1;
                _i /= 10;
            }
            return string(bstr);
        }

    function changeDescription(uint tokenId, string memory description) public returns (uint256){
        require(msg.sender == ownerOf(tokenId));
        tokenIdToDescription[tokenId] = description;
    }
}